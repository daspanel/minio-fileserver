{
	"version": "18",
	"credential": {
		"accessKey": "{{ (datasource "cfg").s3.access_key }}",
		"secretKey": "{{ (datasource "cfg").s3.secret_key }}"
	},
	"region": "{{ (datasource "cfg").s3.region }}",
	"browser": "on",
	"logger": {
		"console": {
			"enable": true
		},
		"file": {
			"enable": false,
			"filename": ""
		}
	},
	"notify": {
		"amqp": {
			"1": {
				"enable": false,
				"url": "amqp://myuser:mypassword@localhost:5672",
				"exchange": "bucketevents",
				"routingKey": "bucketlogs",
				"exchangeType": "fanout",
				"deliveryMode": 0,
				"mandatory": false,
				"immediate": false,
				"durable": false,
				"internal": false,
				"noWait": false,
				"autoDeleted": false
			}
		},
		"nats": {
			"1": {
				"enable": false,
				"address": "",
				"subject": "",
				"username": "",
				"password": "",
				"token": "",
				"secure": false,
				"pingInterval": 0,
				"streaming": {
					"enable": false,
					"clusterID": "",
					"clientID": "",
					"async": false,
					"maxPubAcksInflight": 0
				}
			}
		},
		"elasticsearch": {
			"1": {
				"enable": false,
				"format": "namespace",
				"url": "http://127.0.0.1:9200",
				"index": "bucketevents"
			}
		},
		"redis": {
			"1": {
				"enable": false,
				"format": "namespace",
				"address": "{{ (datasource "cfg").redis.server }}:{{ (datasource "cfg").redis.port }}",
				"password": "{{ (datasource "cfg").redis.password }}",
				"key": "bucketevents"
			}
		},
		"postgresql": {
			"1": {
				"enable": false,
				"format": "namespace",
				"connectionString": "",
				"table": "bucketevents",
				"host": "127.0.0.1",
				"port": "5432",
				"user": "postgres",
				"password": "mypassword",
				"database": "bucketevents_db"
			}
		},
		"kafka": {
			"1": {
				"enable": false,
				"brokers": [
					"localhost:9092"
				],
				"topic": "bucketevents"
			}
		},
		"webhook": {
			"1": {
				"enable": false,
				"endpoint": "http://requestb.in/1525x9n1"
			}
		},
		"mysql": {
			"1": {
				"enable": false,
				"format": "namespace",
				"dsnString": "",
				"table": "minio_images",
				"host": "172.17.0.1",
				"port": "3306",
				"user": "root",
				"password": "password",
				"database": "miniodb"
			}
		}
	}
}

